{
  "name": "gatsby-starter-mate",
  "version": "2.2.1",
  "description": "Gatsby v2 starter to create a top notch portfolio!",
  "main": "index.js",
  "scripts": {
    "build": "cleandir ./.cache && gatsby build",
    "develop": "cleandir ./.cache && gatsby develop",
    "start": "npm run develop",
    "lint": "eslint ./src",
    "setup": "node ./bin/setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmaSuriano/gatsby-starter-mate.git"
  },
  "keywords": [
    "gatsby",
    "starter",
    "contentful",
    "PWA",
    "portfolio"
  ],
  "author": "Ema Suriano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmaSuriano/gatsby-starter-mate/issues"
  },
  "homepage": "https://github.com/EmaSuriano/gatsby-starter-mate#readme",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "clean-dir": "^1.0.3",
    "contentful": "^7.10.0",
    "dotenv": "^7.0.0",
    "envfile": "^3.0.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "gatsby": "2.15.28",
    "gatsby-plugin-google-analytics": "^2.1.19",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-google-gtag": "^1.1.11",
    "gatsby-plugin-gtag": "^1.0.11",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-styled-components": "^3.1.8",
    "gatsby-source-contentful": "^2.1.44",
    "gatsby-source-medium": "^2.1.9",
    "gatsby-transformer-remark": "^2.6.26",
    "inquirer": "^6.5.2",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-fontawesome": "^1.6.1",
    "react-headroom": "^2.2.4",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.2.2",
    "react-pricing-table": "^0.3.0",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^1.3.0",
    "react-text-loop": "^2.1.1",
    "react-tippy": "^1.2.2",
    "rebass": "^3.2.2",
    "styled-components": "^4.4.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "contentful-import": "^7.6.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
